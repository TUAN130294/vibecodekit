{
  "name": "automation-data-analytics-platform",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "init": "node scripts/init.js",
    "init:from-config": "node scripts/init-from-config.js",
    "dev": "tsx watch src/server.ts",
    "dev:web": "npx next dev",
    "dev:lite": "npx next dev",
    "dev:pro": "docker-compose --profile pro up",
    "dev:full": "docker-compose --profile full up",
    "build": "tsc -p tsconfig.json",
    "build:web": "npx next build",
    "start": "node dist/server.js",
    "start:web": "npx next start",
    "deploy:vercel": "vercel --prod",
    "deploy:docker": "docker-compose --profile pro up -d",
    "test": "jest --runInBand",
    "test:e2e": "jest --config jest.config.js --runInBand --testPathPattern=e2e",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "format": "prettier --write \"{src,tests}/**/*.{ts,tsx,js,json,md}\"",
    "worker": "ts-node src/worker.ts || echo \"worker stub\"",
    "doctor": "node scripts/doctor.js",
    "clean": "rm -rf .next node_modules dist .kit-config.json"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.3.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.2.2",
    "stripe": "^14.23.0",
    "typeorm": "^0.3.20",
    "winston": "^3.12.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@next/swc-win32-x64-msvc": "^16.0.8",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^6.0.1",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/stripe": "^8.0.0",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "autoprefixer": "^10.4.19",
    "cross-env": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "next": "^14.2.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "supertest": "^7.1.0",
    "tailwindcss": "^3.4.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.4.5"
  }
}
